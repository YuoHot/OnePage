	<script>
		var proname = "";
		$(function () {
			$("#jc1,#tw1,#al1,#sp1").click(function () {
				$("#jc1,#tw1,#al1,#sp1").each(function () {
					$(this).css("border-bottom","0px");
				});
				$(this).css("border-bottom","2px solid blue");

			});
			$("#logback").hide();
			$(".ail").click(function () {
				var name = $(this).children("div[class='lio']").text();
				var opentype = $(this).children("div[class='lio']").attr("brtag");
				if(name!=proname)
				{	
					if(opentype=="Edit")
					{
						$("#logback").css({"width":"100%","height":"100%"});
						proname = name;
						$("#logback").show(200,function () {
							if(UpdateTxt(name))
							{
								$('body').oneTime('1s', function() {
									$("#logback").hide(200);
								});
							}
						});
					}
					else if(opentype=="OpenLink")
					{
						$("#linkbar").show(200,function(){
							$("#showlinkbar").show();
							$("#showlink").attr("src","http://www.baidu.com");
							$("#showlink").load(function () {
								$("#linkbar").hide(200);
							});
						});
					}
				}
			});	
			$(".al").click(function () {
				$("#logback").css({"width":"100%","height":"100%"});
				$("#logback").show(200,function () {
					var tyt = "<h3>请登录</h3><input type='text' name='username' placeholder='请输入账户名' required autofocus/><input type='password' name='password' placeholder='请输入密码' required autofocus/><div style='height:50px;line-height:50px;font-family:\'微软雅黑\''><input type='checkbox'>记住密码</input></div><button type='button' class='btn btn-success col-lg-12'>登录</button>";
					if(UpdateTxt(tyt))
					{
						$('body').oneTime('1s', function() {
							$("#logback").hide(200);
						});
					}
				});
			});
			$("#goback").click(function () {
				$("#showlinkbar").hide(200);
			});

			function UpdateTxt(name)
			{
				$("#editback").html(name);
				return true;
			}
		});
